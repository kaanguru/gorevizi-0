{
  "expo": {
    "name": "gorevizi-0",
    "slug": "gorevizi-0",
    "version": "1.0.0",
    "newArchEnabled": true,
    "scheme": "gorevizi-0",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/favicon.png"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icons/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fffaec"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#fffaec"
      },
      "package": "com.kaanguru.gorevizi0"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d4cc825f-1292-4258-b372-4c0294a4997c"
      }
    }
  }
}
